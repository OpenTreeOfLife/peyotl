    ],
    "queries": [
        {
            "name": "Harvest recent Apis mellifera",
            "automatic": true,
            "queryType": "study",
            "filters": [
                {
                    "property": "ot:publicationDate",
                    "test": "GREATER THAN",
                    "value": "2013"
                },
                {
                    "property": "ot:focalClade",
                    "test": "WITHIN CLADE",
                    "value": "Apis mellifera"
                }
            ]
        },
        {
            "name": "Find maximum-likelihood trees",
            "automatic": false,
            "queryType": "tree",
            "filters": [
                {
                    "property": "ot:inferenceMethod",
                    "test": "EXACT MATCH",
                    "value": "maximum likelihood"
                }
            ]
        }
    ]
}
